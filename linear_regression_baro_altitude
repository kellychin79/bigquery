CREATE OR REPLACE MODEL `bigdataassignment2-320523.Assignment2_Data.baro_altitude`
OPTIONS
    (model_type='linear_reg',input_label_cols=['baro_altitude']) AS

SELECT  
    baro_altitude,
    altitude, 
    longitude, 
    latitude, 
    vertical_rate, 
    icao24,
    velocity

FROM `bigdataassignment2-320523.Assignment2_Data.assign2_live`
WHERE baro_altitude IS NOT NULL
